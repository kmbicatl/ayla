
							<div class="panel panel-primary">
								<div class="panel-heading">
									<h4 class="icons col-lg-11">Customer</h4>
									<div>
										<div class="make-switch" data-on="warning" data-off="warning">
											<input id="radioClients" type="radio" checked="checked"/>
										</div>
									</div>
								</div>
								<div class="panel-body" ng-controller="ClientsController">
									<form autocomplete="off" class="form-horizontal css-form" id="formClients" name="formClients" novalidate>
										<div class="form-group">
											<label class="control-label col-lg-4">Name</label>
											<div class="col-lg-4">
												<input type="formatedNames" id="name" name="name" ng-model="client.name" class="form-control" required/>
											</div>
										</div>		
										<div class="form-group">
											<label class="control-label col-lg-4">Status</label>
											<div class="col-lg-4">
												<select id="status" name="status" ng-model="client.status" class="form-control" required>
													<option value="1">Active</option>
													<option value="2">InActive</option>
												</select>
											</div>
										</div>										
										<div class="form-actions no-margin-bottom" style="text-align:center;">
											<input type="reset" class="btn btn-warning btn-lg" value="Reset" />
											<input type="submit" class="btn btn-primary btn-lg " value="Submit"/>
										</div>

									</form>
									<form id="formClientsList">
										<div class="table-responsive">
											<table class="table table-striped table-bordered table-hover">
												<thead>
													<tr>
														<th>Customer Id</th>
														<th>Name</th>
														<th>Status</th>
														<th>Created</th>
													</tr>
												</thead>
												<tbody>
												</tbody>
											</table>
										</div>
									</form>
								</div>							
							</div>
							<script>
								$(function(){
									clientsController();
								});
							</script>